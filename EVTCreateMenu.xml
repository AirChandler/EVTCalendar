						<Offset>
							<AbsDimension x="-16" y="16"/>
					</Offset>
				</Anchor>
			</Anchors>
				<Scripts>
					<OnClick>
						PlaySound("gsTitleOptionExit");
						HideUIPanel(EVTFrameCreatePopup);
